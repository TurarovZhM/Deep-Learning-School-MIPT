   "source": [
    "def mult(a, b):\n",
    "    m = a.shape[0]\n",
    "    n = a.shape[1]\n",
    "    l = b.shape[1]\n",
    "    c = np.zeros((m,l))\n",
    "    for i in range(0,m):\n",
    "        for j in range(0,l):\n",
    "            for k in range(0,n):\n",
    "                c[i,j] += a[i,k] * b[k,j]\n",
    "    return c"
   ]
  },
