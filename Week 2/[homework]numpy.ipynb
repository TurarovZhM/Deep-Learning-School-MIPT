    "# функция без NumPy\n",
    "def stupid_sec_av(A):\n",
    "    S = [0 for i in range(len(A))]\n",
    "    for i in range(0, len(A)):\n",
    "        if i != 0:\n",
    "            S[i] = S[i-1] + A[i]\n",
    "        else:\n",
    "            S[i] = A[i]\n",
    "\n",
    "    for i in range(len(A)):\n",
    "        S[i] /= (i+1)\n",
    "\n",
    "    return S\n",
